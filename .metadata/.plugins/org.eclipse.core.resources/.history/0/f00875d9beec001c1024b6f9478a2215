def findLUSlength(self, a: str, b: str) -> int:
    a_subs, b_subs = [], []
    len_a, len_b = len(a), len(b)
    
    for i in range(len_a):
        for j in range(i+1, len_a+1):
            a_subs.append(a[i:j])
    for i in range(len_b):
        for j in range(i+1, len_b+1):
            b_subs.append(b[i:j])
    
    result = -1
    for sub in a_subs:
        if sub not in b_subs and len(sub) > result:
            result = len(sub)
    for sub in b_subs:
        if sub not in a_subs and len(sub) > result:
            result = len(sub)
            
    return result